<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.kh.mappers.RoomOptionMapper">

	<!-- select RoomOption -->
	<select id="getRoomOptionList" resultType="RoomOptionVo">
		SELECT column_name
		FROM user_tab_columns
		WHERE table_name = 'TBL_ROOM_OPTION' and column_name != 'ROOM_NUM'
	</select>
  
</mapper>